--------------------------------------------------------------------------------
psp:Context psp:Actie
	psp:rolInContext => psp:actorView
	psp:rolInContext => psp:werkwoord
	psp:rolInContext => psp:objectView

psp:Rol psp:actorView
	extern psp:isFunctioneel = true
	psp:mogelijkeBinding => psp:View

psp:Rol psp:werkwoord
	extern psp:isFunctioneel = true
	psp:mogelijkeBinding => psp:Werkwoord

psp:Rol psp:objectView
	extern psp:isFunctioneel = true
	psp:mogelijkeBinding => psp:View

psp:Context psp:Werkwoord

psp:Context psp:Beheert
	psp:aspect => psp:Werkwoord

psp:Context psp:Raadpleegt
	psp:aspect => psp:Werkwoord

psp:Context psp:CreÃ«ert
	psp:aspect => psp:Werkwoord

psp:Context psp:Verwijdert
	psp:aspect => psp:Werkwoord

--------------------------------------------------------------------------------
-- FUNCTION
--------------------------------------------------------------------------------
psp:Context psp:Function
	psp:rolInContext =>
		psp:Rol $domain
			extern psp:isFunctioneel = true
			psp:mogelijkeBinding => psp:ElkType
	psp:rolInContext =>
		psp:Rol $range
			extern psp:isFunctioneel = false
			psp:mogelijkeBinding => psp:ElkType

psp:Context psp:ElkType

psp:Context psp:Constraint
	psp:aspect => psp:Function
	psp:rolInContext =>
		psp:Rol $range
			extern psp:isFunctioneel = true
			psp:aspectRol => psp:Function$range
			psp:mogelijkeBinding => psp:Boolean

