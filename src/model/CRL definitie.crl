	psp:mogelijkeBinding => psp:Property
psp:Rol psp:rolProperty
	extern psp:isFunctioneel = false
	psp:mogelijkeBinding => psp:Property
psp:Rol psp:rolInContext
	extern psp:isFunctioneel = false
	psp:mogelijkeBinding => psp:Rol
psp:Rol psp:viewInRol
	extern psp:isFunctioneel = false
	psp:mogelijkeBinding => psp:View
psp:Rol psp:interneView
	extern psp:isFunctioneel = false
	psp:mogelijkeBinding => psp:View
psp:Rol psp:externeView
	extern psp:isFunctioneel = false
	psp:mogelijkeBinding => psp:View
psp:Rol psp:prototype
	extern psp:isFunctioneel = true
	psp:mogelijkeBinding => psp:Context
psp:Rol psp:aspect
	extern psp:isFunctioneel = false
	psp:mogelijkeBinding => psp:Context
psp:Rol psp:actie
